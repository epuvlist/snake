    "version": "0.2.0",
    "configurations": [
        {
            "name": "C++ Launch",
            "type": "cppdbg",
            "cwd": "${fileDirname}",
            "request": "launch",
            "targetArchitecture":"arm64",
            "program": "${fileDirname}/${fileBasenameNoExtension}",
            "args": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "logging": {
              "moduleLoad": false,
              "trace": true
            },
            "preLaunchTask": "C/C++: g++ build active file"
          }
    ]
}
